{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build: DEBUG - rustAOCean2020",
			"type": "shell",
			"problemMatcher": [
				"$rustc"
			],
			"command": "cargo",
			"args": [
				"build"
			]
		},
		{
			"label": "Build: RELEASE - rustAOCean2020",
			"type": "shell",
			"problemMatcher": [
				"$rustc"
			],
			"command": "cargo",
			"args": [
				"build",
				"--release"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Run: RELEASE - rustAOCean2020",
			"type": "shell",
			"problemMatcher": [
				"$rustc"
			],
			"command": "cargo",
			"args": [
				"run",
				"--release"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}